<h1> Editar Perfil</h1>

<%= simple_form_for [:partners, @information] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :full_name,
                autofocus: true,
                input_html: { autocomplete: "title" }%>

    <%= f.input :language %>
    <%= f.input :contact_phone %>
    <%= f.input :cpf %>
    <%= f.input :cnpj %>
    <%= f.input :cep %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :address %>
    <%= f.input :district %>
    <%= f.input :address_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Criar" %>
  </div>
<% end %>

<%= link_to "Voltar", partners_dashboard_path %>